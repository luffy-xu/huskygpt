{
  "name": "huskygpt",
  "version": "0.1.6",
  "description": "Automatically generate unit tests or review your code using openai chatgpt",
  "main": "lib/index.js",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del build",
    "prebuild": "run-s clean",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "test": "run-p test:*",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "files": [
    "build",
    "bin"
  ],
  "bin": {
    "huskygpt": "./bin/cli.js"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luffy-xu/huskygpt"
  },
  "author": "https://github.com/luffy-xu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luffy-xu/huskygpt/issues"
  },
  "homepage": "https://github.com/luffy-xu/huskygpt#readme",
  "dependencies": {
    "@babel/generator": "^7.21.3",
    "@babel/parser": "^7.21.3",
    "@babel/traverse": "^7.21.3",
    "chalk": "^5.2.0",
    "chatgpt": "^5.1.4",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "isomorphic-fetch": "^3.0.0",
    "openai": "^3.2.1",
    "ora": "^5.4.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "openai",
    "chatgpt",
    "chat",
    "gpt",
    "gpt-3",
    "gpt3",
    "gpt4",
    "conversation",
    "conversational ai",
    "ai",
    "test",
    "auto",
    "generate",
    "gen",
    "jest",
    "unit test",
    "code review"
  ],
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^18.15.10",
    "del-cli": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  }
}
